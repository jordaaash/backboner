{
    "node":      true,
    "browser":   true,
    "eqnull":    true,
    "nonew":     true,
    "curly":     true,
    "latedef":   "nofunc",
    "unused":    "strict",
    "strict":    true,
    "noarg":     true,
    "trailing":  true,
    "indent":    2,
    "forin":     true,
    "noempty":   true,
    "quotmark":  "single",
    "supernew":  true,
    "maxerr":    100,
    "eqeqeq":    true,
    "undef":     true,
    "bitwise":   true,
    "newcap":    true,
    "immed":     true,
    "camelcase": true,
    "maxlen":    80,
    "nonbsp":    true,
    "freeze":    true,
    "sub":       true,
    "-W093":     false,
    "-W068":     false,
    "-W089":     false,
    "globals":   {
        "process":       true,
        "setTimeout":    true,
        "require":       true,
        "console":       true,
        "module":        true,
        "GLOBAL":        true,
        "describe":      true,
        "before":        true,
        "it":            true,
        "clientSockets": true,
        "__dirname":     true
    }
}
